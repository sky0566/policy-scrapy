<script>
function gotoPage(currentSelect) {
	var pn = currentSelect;
	if(isNaN(pn)){
		 alert('必须输入数字!');    
	}else{
		selectPage(pn);
	}
	
 	
}

function selectPage(pn){
	var page = 34;
	if(pn>page){
		pn=page;
	}
	if(pn<1){
		pn = 1;
	}
	$("#currentPage").val(pn);
	$("#listForm").submit();
}
</script>
