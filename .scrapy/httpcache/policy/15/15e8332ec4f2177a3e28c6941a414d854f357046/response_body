<script type="text/javascript">
function serch(){
	var serchid = $("#serchid").val();
	var gjz = $.trim($("#gjz").val());
	var url= "";
	if(gjz =="")
	    url="/result?bureauId=41&typeId=1&ml_Id=1";
	else
		url="/serch?serchid="+serchid+"&gjz="+gjz+"&bureauId=41";
	document.all.listForm.action="";
    document.all.listForm.action=url;
	$("#listForm").submit();
}
(function(){document.all.listForm.action="/result?bureauId=41&typeId=1&ml_Id=1";})
</script>
